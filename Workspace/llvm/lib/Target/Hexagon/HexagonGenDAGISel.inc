  return XformM5ToU5Imm(imm);

  }
  case 9: {  // DEC_CONST_BYTE
    ConstantSDNode *N = cast<ConstantSDNode>(V.getNode());

   // Return the byte immediate const-1 as an SDNode.
   int32_t imm = N->getSExtValue();
   return XformU7ToU7M1Imm(imm);

