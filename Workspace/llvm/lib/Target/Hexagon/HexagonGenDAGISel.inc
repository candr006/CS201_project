bool CheckPatternPredicate(unsigned PredNo) const override {
  switch (PredNo) {
  default: llvm_unreachable("Invalid predicate in table?");
  case 0: return (Subtarget.hasV4TOps()) && (Subtarget.useMemOps());
  case 1: return (Subtarget.hasV4TOps());
  case 2: return (Subtarget.hasV5TOps());
  case 3: return (!Subtarget.hasV4TOps());
  case 4: return (Subtarget.hasV5TOps()) && (Subtarget.modeIEEERndNear());
  case 5: return (Subtarget.hasV2TOpsOnly());
  case 6: return (Subtarget.hasV3TOps());
  }
}

